{
  "name": "Monitoring solution",
  "description": "Bachelor's paper project",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./exam-icon.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["storage", "tabs", "scripting"]
}
